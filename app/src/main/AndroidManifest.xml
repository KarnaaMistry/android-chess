<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidchess10">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round_2"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidChess10">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="@string/replaytitle" android:name=".ReplayListActivity">
        </activity>
        <activity android:label="@string/replaydisplay" android:name=".ReplayViewActivity">
        </activity>
    </application>

</manifest>